    "recoil": "^0.7.4",
    "storybook-addon-designs": "^6.3.1",
    "storybook-addon-react-router-v6": "^0.1.11",
    "storybook-addon-recoil-flow": "^1.3.1",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "styled-components": "^5.3.5",
    "swiper": "^8.3.2",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "wretch": "^1.7.10",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/faker": "^5.5.9",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-table": "^7.7.12",
    "@types/styled-components": "^5.1.25",
    "@types/yup": "^0.29.11",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cypress": "^10.3.1",
    "dotenv-cli": "^4.1.0",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "prop-types": "^15.8.1",
    "webpack": "^5.74.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "build:production": "env-cmd -f ./.env.production npm run-script build",
    "build:staging": "env-cmd -f ./.env.staging npm run-script build",
    "build:dev": "dotenv -e .env.development react-scripts build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cypress": "npx cypress open --component"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.tsx"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
